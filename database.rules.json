{
  "rules": {
    "sensors": {
      ".read": true,
      ".write": true,
      "latest": {
        ".indexOn": [
          "timestamp"
        ]
      },
      "history": {
        ".indexOn": [
          "timestamp"
        ]
      }
    },
    "BlueSentinel": {
      ".read": true,
      ".write": true
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}